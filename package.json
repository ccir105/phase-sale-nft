{
  "name": "bubblebot-nft",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "accounts": "npx hardhat accounts",
    "compile": "npx hardhat compile",
    "lint": "solhint -c solhint.json --ignore-path .solhintignore -w 0 ./contracts/*.sol",
    "test": "npx hardhat test",
    "test:dotonly": "dot-only-hunter test",
    "coverage": "npx hardhat coverage",
    "clean": "npx hardhat clean",
    "chain": "npx hardhat node",
    "verify:testnet": "npx hardhat verify --network testnet --constructor-args args.js 0xC122e33187652Ae42e1781343909ECA697a280A1",
    "verify:mainnet": "npx hardhat verify --network mainnet --constructor-args args.js 0x2A644467FfD0BAfd597fE00711EDD074C1097305",
    "format:fix": "npx prettier --write 'contracts/*.sol'",
    "prettier:check": "npx prettier --check 'contracts/*'",
    "issues": "slither . --solc-disable-warnings 2>&1",
    "create-imx:testnet": "npx hardhat --network testnet create-project",
    "create-imx:live": "npx hardhat --network live create-project",
    "deplpoy:live": "npx hardhat --network live run scripts/deployBubbleBot.ts",
    "verify:wl": "npx hardhat --network testnet verify-wl --address 0xc071d19126979db587c0562b993b2a61d145f25c --amount 4"
  },
  "author": "PeanutGames Team",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.22",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "chai": "^4.3.4",
    "dot-only-hunter": "^1.0.3",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.3",
    "hardhat-gas-reporter": "^1.0.6",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2",
    "web3": "^1.6.1"
  },
  "dependencies": {
    "@imtbl/core-sdk": "^1.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "@openzeppelin/hardhat-upgrades": "^1.14.0",
    "@pinata/sdk": "^2.1.0",
    "aws-sdk": "^2.1301.0",
    "axios": "^1.2.4",
    "erc20-snapshot": "^1.0.2",
    "erc721a": "^4.2.3",
    "ethers-multicall": "^0.2.3",
    "ipfs-http-client": "56.0.2",
